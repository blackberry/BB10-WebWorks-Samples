<widget id="com.@@@@@@.pushinitiator" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Push Initiator</name>
    
	<description>
        A sample WebWorks application showing BlackBerry Push initator integration.
    </description>
	
    <author email="eoros@blackberry.com" href="http://developer.blackberry.com/html5">
        Erik Oros
    </author>
	
    <content src="index.html" />
    
	<access origin="*" />
	<access origin="https://pushapi.eval.blackberry.com" subdomains="true" />
    
	<preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
</widget>
